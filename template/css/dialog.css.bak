@charset "utf-8";.fl{float:left;}
.fr{float:right;}
.clear{clear:both;}
.bold{font-weight:700;}
.f11{font-size:13px;}
.f14{font-size:16px;}
.tl{text-align:left;}
.tc{text-align:center;}
.a-right,.tr{text-align:right;}
.nobr,.nowrap{white-space:nowrap;word-wrap:normal;}
.loading{padding:60px 20px;text-align:center;}
.loading i{display:inline-block;width:28px;height:28px;background:url(/template/images/confrim/loading.gif) no-repeat;vertical-align:middle;}
.loading span{margin-left:.8em;color:#999;}
.ds_tips{border:1px solid #d5d5d5;border-bottom:3px solid #4367af;border-radius:10px;background:#fafafa;box-shadow:0 1px 4px rgba(0,0,0,.24);}
.ds_tips .ds_tips_content{padding:5px 10px;border:1px solid #fff;border-radius:9px;}
.ds_tips .ds_tips_arrow,.ds_tips .ds_tips_arrow b{position:absolute;top:0;left:50%;margin:-8px 0 0 -8px;width:0;height:0;border-color:#d5d5d5 transparent;border-style:solid dashed;border-width:0 8px 8px;background:0 0;font-size:0;}
.ds_tips .ds_tips_arrow b{left:0;margin:1px 0 0 -8px;border-bottom-color:#fafafa;}
.placeholder,:-moz-placeholder,:-ms-placeholder,::-webkit-input-placeholder,::placeholder{color:#bbb;}
.ds_dialog{position:absolute;top:44%;left:44%;z-index:1999;outline:0;color:#333;font:15px/1.6 "Microsoft Yahei",Arial;}
.ds_dialog,.ds_dialog_border,.ds_dialog_panel .ds_dialog_title,.ds_dialog_title h3{margin:0;padding:0;text-align:left;}
.ds_dialog_border,.ds_dialog_panel{margin:0;padding:0;min-width:260px;min-height:155px;border-collapse:collapse;}
.ds_dialog_bc,.ds_dialog_bl,.ds_dialog_br,.ds_dialog_ml,.ds_dialog_mr,.ds_dialog_tc,.ds_dialog_tl,.ds_dialog_tr{margin:0;padding:0;background:#000;background:rgba(0,0,0,.5);filter:Alpha(opacity=50);}
.ds_dialog_bl,.ds_dialog_br,.ds_dialog_tl,.ds_dialog_tr{width:8px;height:8px;}
.ds_dialog_inner{position:relative;min-width:260px;min-height:155px;background:#fff;}
.ds_dialog_header{vertical-align:top;}
.ds_dialog_title{height:38px;background:#0baca8;}
.ds_dialog_title h3{padding:0 30px 0 16px;height:38px;color:#fff;white-space:nowrap;font-weight:400;font-size:16px;line-height:38px;cursor:default;}
.ds_dialog_active .ds_dialog_title h3{color:#FFF;}
.ds_dialog_close{position:absolute;top:6px;right:10px;width:24px;height:24px;}
.ds_dialog_close a{display:block;overflow:hidden;height:24px;color:#FFF;text-align:center;text-decoration:none;text-shadow:0 -1px 0 #395ba1;font:26px/24px Arial,Helvetica;}
.ds_dialog_close a:hover{color:#DDD;text-decoration:none;}
.ds_dialog_icon{padding:10px 0 10px 10px;vertical-align:middle;}
.ds_dialog_icon .ds_dialog_icon_bg{display:block;width:40px;height:40px;background:none no-repeat 50% 50%;}
.ds_dialog_main{vertical-align:middle;}
.ds_dialog_footer{vertical-align:bottom;}
.ds_dialog .ds_dialog_buttons{padding:8px;border-top:1px solid #F0F0F0;background:#FAFAFA;text-align:right;white-space:nowrap;font-size:0;}
.ds_dialog_buttons button{display:inline-block;margin-right:20px;padding:0 32px;height:32px;border:0;border-radius:18px;background:#ececec;color:#9e9e9e;text-decoration:none!important;font-size:14px;font-family:"微软雅黑";line-height:32px;cursor:pointer;}
.ds_dialog_buttons button::-moz-focus-inner{margin:0;padding:0;border:0;}
.ds_dialog_buttons button:hover{background:#F3F3F3;background:-moz-linear-gradient(top,#EEE 0,#F3F3F3 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#EEE),color-stop(100%,#F3F3F3));background:-webkit-linear-gradient(top,#EEE 0,#F3F3F3 100%);background:-o-linear-gradient(top,#EEE 0,#F3F3F3 100%);background:-ms-linear-gradient(top,#EEE 0,#F3F3F3 100%);background:linear-gradient(to bottom,#EEE 0,#F3F3F3 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#EEEEEE', endColorstr='#F3F3F3', GradientType=0);}
.ds_dialog_buttons button:focus{border-color:#426DC9;box-shadow:0 0 5px #FFBA08;}
.ds_dialog_buttons button:active{border-color:#b4b4b4;background:-moz-linear-gradient(top,#DDD 0,#FFF 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#DDD),color-stop(100%,#FFF));background:-webkit-linear-gradient(top,#DDD 0,#FFF 100%);background:-o-linear-gradient(top,#DDD 0,#FFF 100%);background:-ms-linear-gradient(top,#DDD 0,#FFF 100%);background:linear-gradient(to bottom,#DDD 0,#FFF 100%);box-shadow:inset 0 1px 5px #FFF;filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#DDDDDD', endColorstr='#FFFFFF', GradientType=0);}
.ds_dialog_buttons .ds_dialog_yes{display:inline-block;margin-right:20px;padding:0 32px;height:32px;border:0;border-radius:18px;background:#08a09d;color:#fff;text-decoration:none!important;font-size:14px;font-family:"微软雅黑";line-height:32px;cursor:pointer;}
.ds_dialog_buttons .ds_dialog_yes:hover{background:#0baca8;background:-moz-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#0baca8),color-stop(100%,#0baca8));background:-webkit-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-o-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-ms-linear-gradient(top,#0baca8 0,#0baca8 100%);background:linear-gradient(to bottom,#0baca8 0,#0baca8 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#0baca8', endColorstr='#0baca8', GradientType=0);}
.ds_dialog_buttons .ds_dialog_yes:active{border-color:#0baca8;background:-moz-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#0baca8),color-stop(100%,#0baca8));background:-webkit-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-o-linear-gradient(top,#0baca8 0,#0baca8 100%);background:-ms-linear-gradient(top,#0baca8 0,#0baca8 100%);background:linear-gradient(to bottom,#0baca8 0,#0baca8 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#0baca8', endColorstr='#0baca8', GradientType=0);}
.ds_dialog_buttons .disabled,.ds_dialog_buttons .disabled:active,.ds_dialog_buttons .disabled:focus,.ds_dialog_buttons .disabled:hover,.ds_dialog_buttons button:disabled{outline:0;border-color:#C6C6C6;background:#E9E9E9;box-shadow:inset 0 0 1px #FCFCFC;color:#AAA;text-shadow:0 1px 0 #fff;cursor:default;pointer-events:none;filter:none;}
